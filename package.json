{
  "name": "@xops.net/expedition",
  "version": "1.0.2",
  "description": "Expedition is a block explorer for the Ethereum Stack",
  "keywords": [
    "typescript",
    "react",
    "dapp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xops/expedition.git"
  },
  "scripts": {
    "explorer": "cross-env GENERATE_SOURCEMAP=false REACT_APP_ETH_RPC_URL=https://rpc.api.moonbeam.network/ react-app-rewired start",
    "build": "react-app-rewired build && cp build/index.html build/404.html",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test",
    "lint": "tslint -p tsconfig.json",
    "version": "cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'"
  },
  "dependencies": {
    "@etclabscore/eserialize": "^1.1.2",
    "@etclabscore/ethereum-json-rpc": "^1.3.13",
    "@etclabscore/ethereum-json-rpc-specification": "git+https://github.com/etclabscore/ethereum-json-rpc-specification.git",
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/styles": "^4.11.5",
    "@monaco-editor/react": "^2.6.1",
    "@qiwi/semantic-release-gh-pages-plugin": "^5.2.5",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "bignumber.js": "^9.1.1",
    "buffer": "^6.0.3",
    "ethereumjs-util": "^6.2.1",
    "ethjs-unit": "^0.1.6",
    "history": "^4.10.1",
    "i18next": "^19.0.0",
    "i18next-browser-languagedetector": "^3.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "qs": "^6.11.0",
    "query-string": "^6.14.1",
    "raf": "^3.4.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^12.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "reusable": "^1.0.1",
    "sass": "^1.58.3",
    "semantic-release": "^20.1.1",
    "use-dark-mode": "^2.3.1",
    "use-interval": "^1.4.0",
    "use-query-params": "^0.6.0",
    "use-react-router": "^1.0.7",
    "usehooks-ts": "^2.9.1",
    "victory": "^36.6.8"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@types/bignumber.js": "^4.0.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^14.18.36",
    "@types/react": "^16.14.35",
    "@types/react-dom": "^16.9.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/victory": "^31.0.22",
    "cross-env": "^7.0.3",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.9.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
