{
  "name": "@xops.net/expedition",
  "version": "1.0.2",
  "description": "Expedition is a block explorer for the Ethereum Stack",
  "keywords": [
    "typescript",
    "react",
    "dapp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xops/expedition.git"
  },
  "scripts": {
    "explorer": "cross-env GENERATE_SOURCEMAP=false REACT_APP_ETH_RPC_URL=https://rpc.api.moonbeam.network/ react-app-rewired start",
    "build": "react-app-rewired build && cp build/index.html build/404.html",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test",
    "lint": "tslint -p tsconfig.json",
    "version": "cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'"
  },
  "dependencies": {
    "@etclabscore/eserialize": "^1.0.2",
    "@etclabscore/ethereum-json-rpc": "^1.3.13",
    "@etclabscore/ethereum-json-rpc-specification": "git+https://github.com/etclabscore/ethereum-json-rpc-specification.git",
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.2",
    "@monaco-editor/react": "^2.1.1",
    "@qiwi/semantic-release-gh-pages-plugin": "^1.14.2",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/github": "^5.5.8",
    "@semantic-release/npm": "^5.3.5",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "bignumber.js": "^9.0.2",
    "buffer": "^6.0.3",
    "ethereumjs-util": "^6.1.0",
    "ethjs-unit": "^0.1.6",
    "history": "^4.9.0",
    "i18next": "^17.3.1",
    "i18next-browser-languagedetector": "^3.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.0",
    "node-html-parser": "^6.1.5",
    "qs": "^6.11.0",
    "query-string": "^6.8.3",
    "raf": "^3.4.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-i18next": "^10.13.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "reusable": "^1.0.1",
    "sass": "^1.57.1",
    "semantic-release": "^15.13.32",
    "semantic-release-docker": "^2.2.0",
    "use-dark-mode": "^2.3.1",
    "use-interval": "^1.1.0",
    "use-query-params": "^0.4.6",
    "use-react-router": "^1.0.7",
    "usehooks-ts": "^2.9.1",
    "victory": "^33.1.7"
  },
  "devDependencies": {
    "@semantic-release/exec": "^3.3.8",
    "@types/bignumber.js": "^4.0.3",
    "@types/jest": "^21.1.10",
    "@types/lodash": "^4.14.191",
    "@types/node": "^14.11.11",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.18",
    "@types/react-router-dom": "^5.1.9",
    "@types/victory": "^31.0.22",
    "cross-env": "^7.0.3",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.8.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.9.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
